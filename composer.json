{
    "name": "christopherspenn/crispy-seo",
    "description": "Comprehensive SEO plugin with meta tags, JSON-LD, sitemaps, redirects, internal links, and analytics",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Christopher S. Penn",
            "homepage": "https://christopherspenn.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "CrispySEO\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
